<Window x:Class="Projet.AjouterObjetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Ajouter un objet" Height="300" Width="300" WindowStartupLocation="CenterOwner">
    <StackPanel Margin="10">
        <TextBlock Text="Nom" />
        <TextBox x:Name="NomTextBox" />

        <TextBlock Text="Type Bois" Margin="0,10,0,0"/>
        <TextBox x:Name="TypeBoisTextBox" />

        <TextBlock Text="Prix (€)" Margin="0,10,0,0"/>
        <TextBox x:Name="PrixTextBox" />

        <TextBlock Text="Usage" Margin="0,10,0,0"/>
        <TextBox x:Name="UsageTextBox" />

        <Button Content="Valider" Margin="0,15,0,0" Click="Valider_Click"/>
    </StackPanel>
</Window>
